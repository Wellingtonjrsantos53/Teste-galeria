{
  "version": 2,
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/create_payment",
      "methods": [
        "POST"
      ],
      "dest": "api.js" // Apontando para o novo nome do arquivo
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1" 
    }
  ]
}